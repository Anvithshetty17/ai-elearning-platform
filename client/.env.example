# Environment Configuration for React App

# API Configuration
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_ML_API_URL=http://localhost:5001/api

# App Configuration
REACT_APP_NAME=AI E-Learning Platform
REACT_APP_VERSION=1.0.0
REACT_APP_DESCRIPTION=AI-powered E-Learning Platform with automated lecture generation

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_PWA=true
REACT_APP_ENABLE_NOTIFICATIONS=true
REACT_APP_ENABLE_DARK_MODE=true

# External Services
REACT_APP_CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name

# Performance Configuration
REACT_APP_LAZY_LOADING=true
REACT_APP_IMAGE_OPTIMIZATION=true

# Development Configuration
REACT_APP_DEBUG=false
REACT_APP_LOG_LEVEL=error

# Social Media Links
REACT_APP_GITHUB_URL=https://github.com/Anvithshetty17/ai-elearning-platform
REACT_APP_LINKEDIN_URL=https://linkedin.com/in/your-profile
REACT_APP_TWITTER_URL=https://twitter.com/your-handle

# Contact Information
REACT_APP_SUPPORT_EMAIL=support@your-domain.com
REACT_APP_CONTACT_PHONE=+1-555-0123

# Legal Pages
REACT_APP_PRIVACY_POLICY_URL=/privacy-policy
REACT_APP_TERMS_OF_SERVICE_URL=/terms-of-service